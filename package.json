    "bip84": "^0.2.7",
    "bitcoin-ops": "^1.4.1",
    "bitcoinjs-lib": "^5.2.0",
    "bn.js": "^5.1.3",
    "crypto-js": "^4.0.0",
    "eth-object": "https://github.com/syscoin/eth-object.git",
    "eth-proof": "^2.1.6",
    "node-localstorage": "^2.1.6",
    "syscointx-js": "^1.0.106",
    "trezor-connect": "^8.1.29",
    "varuint-bitcoin": "^1.1.2",
    "web3": "^1.4.0"
  }
