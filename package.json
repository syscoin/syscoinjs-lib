    "browserify": "browserify index.js -s syscoinjs -o syscoinjs-lib.browser.js"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "nyc": "^15.1.0",
    "standard": "*",
    "tape": "^5.0.1"
  },
  "dependencies": {
    "@trezor/utxo-lib": "^1.0.7",
